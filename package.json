{
  "name": "handel-codepipeline",
  "version": "0.10.9",
  "description": "Creates CodePipelines running Handel as the deploy action",
  "scripts": {
    "lint": "tslint -p tsconfig.json -t stylish",
    "clean": "node npm-scripts.js clean",
    "copyNonTsFiles": "node npm-scripts.js copyNonTsFiles",
    "compile": "echo Compiling && tsc && echo Finished compile",
    "build": "npm run clean && npm run compile && npm run copyNonTsFiles",
    "test": "./node_modules/mocha/bin/mocha -r ts-node/register 'test/**/*.ts' && npm run build && ./node_modules/mocha/bin/mocha 'test/**/*.js'"
  },
  "bin": {
    "handel-codepipeline": "./bin/handel-codepipeline"
  },
  "author": "David Woodruff",
  "license": "Apache-2.0",
  "dependencies": {
    "@byu-oit/live-plugin-manager": "^1.0.1",
    "archiver": "^1.3.0",
    "async": "^2.6.1",
    "aws-sdk": "^2.332.0",
    "handel": "^0.24.1",
    "handlebars": "^4.0.12",
    "inquirer": "^3.3.0",
    "js-yaml": "^3.12.0",
    "minimist": "^1.2.0",
    "uuid": "^3.3.2",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/sinon-chai": "^2.7.33",
    "@types/archiver": "^2.1.2",
    "@types/async": "^2.0.49",
    "@types/chai": "^4.1.6",
    "@types/handlebars": "^4.0.39",
    "@types/inquirer": "0.0.35",
    "@types/js-yaml": "^3.11.2",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.10.36",
    "@types/sinon": "^2.3.7",
    "@types/uuid": "^3.4.4",
    "@types/winston": "^2.4.4",
    "chai": "^3.5.0",
    "fs-extra": "^4.0.2",
    "mocha": "^5.2.0",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.12.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  }
}
