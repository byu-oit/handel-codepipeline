{
  "name": "handel-codepipeline",
  "version": "0.8.7",
  "description": "Creates CodePipelines running Handel as the deploy action",
  "scripts": {
    "lint": "tslint -p tsconfig.json -t stylish",
    "build": "gulp build",
    "test": "./node_modules/mocha/bin/mocha -r ts-node/register 'test/**/*.ts' && gulp build && ./node_modules/mocha/bin/mocha 'test/**/*.js'",
    "test-coverage": "nyc npm test"
  },
  "bin": {
    "handel-codepipeline": "./bin/handel-codepipeline"
  },
  "author": "David Woodruff",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/deep-equal": "^1.0.1",
    "@types/sinon-chai": "^2.7.29",
    "archiver": "^1.3.0",
    "async": "^2.6.0",
    "aws-sdk": "^2.160.0",
    "handel": "^0.20.9",
    "handlebars": "^4.0.11",
    "inquirer": "^3.3.0",
    "js-yaml": "^3.7.0",
    "minimist": "^1.2.0",
    "uuid": "^3.1.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/archiver": "^2.0.1",
    "@types/async": "^2.0.45",
    "@types/chai": "^4.0.4",
    "@types/handlebars": "^4.0.36",
    "@types/inquirer": "0.0.35",
    "@types/js-yaml": "^3.10.1",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^2.2.44",
    "@types/node": "^8.0.47",
    "@types/sinon": "^2.3.7",
    "@types/uuid": "^3.4.3",
    "@types/winston": "^2.3.7",
    "aws-sdk-mock": "^1.6.1",
    "chai": "^3.5.0",
    "deep-equal": "^1.0.1",
    "eslint": "^3.15.0",
    "eslint-plugin-mocha": "^4.8.0",
    "fs-extra": "^4.0.2",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.2.3",
    "mocha": "^3.2.0",
    "nyc": "^11.3.0",
    "run-sequence": "^2.2.0",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.12.0",
    "source-map": "^0.6.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.7.1"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  }
}
